pipeline{
    agent any
    environment{
            RELEASE='20.4'
    }
    stages{

    stage('Build') {
        agent any
        environment{
            LOG_LEVEL='INFO'
        }
        steps {
            echo "Release ${RELEASE} i log level je ${LOG_LEVEL}"
        }
    }
    stage('Test') {
        echo "Log level je ${LOG_LEVEL} i Release je ${RELEASE}"
    }
}
}